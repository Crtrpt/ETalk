<Window x:Class="ETalk_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ETalk_Client"
        mc:Ignorable="d"
        Title="ETalk v0.0.1" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="username_block" HorizontalAlignment="Left" Margin="273,151,0,0" TextWrapping="Wrap" Text="username:" VerticalAlignment="Top"/>
        <TextBlock x:Name="password_block" HorizontalAlignment="Left" Margin="275,197,0,0" TextWrapping="Wrap" Text="password:" VerticalAlignment="Top"/>
        <TextBox x:Name="username" HorizontalAlignment="Left" Height="23" Margin="346,147,0,0" TextWrapping="Wrap" Text="guest" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="346,197,0,0" VerticalAlignment="Top" Width="120" Height="24" Password="123456"/>
        <Button x:Name="login_btn" Content="login" HorizontalAlignment="Left" Margin="391,246,0,0" VerticalAlignment="Top" Width="75" Click="login_btn_Click"/>
    </Grid>
</Window>
